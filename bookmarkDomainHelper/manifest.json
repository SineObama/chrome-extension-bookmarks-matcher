{
  "name": "智能书签域名识别",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "浏览器收藏夹（书签）助手，智能判断域名是否已有收藏。",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "page_action": {
    "default_icon": "icon.png"
  },
  "background": {
    "persistent": false,
    "scripts": [
      "bookmarkIndexMap.js",
      "background.js"
    ]
  },
  "permissions": [
    "bookmarks",
    "contextMenus",
    "tabs"
  ]
}